#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <string.h>

int main()
{
	char doc[2500], target[50];
	int count = 0;
	char *cur;
	
	gets(doc);
	gets(target);
	
	cur = strstr(doc, target);
	while (cur)
	{
		count++;
		cur = strstr(cur + strlen(target), target);
	}
	
	printf("%d\n", count);
	
	system("pause");
}
