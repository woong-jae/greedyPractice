#include <stdio.h>
#include <string.h>
#include <stdlib.h>


void makeThirty(char num[]);
int compare(const void *a, const void *b);
int main()
{
	char num[100000];
	gets(num);
	makeThirty(num);
}

void makeThirty(char num[])
{
	int result = 0;
	int sum = 0;
	qsort(num, strlen(num), sizeof(char), compare);

	for (int i = 0; i < strlen(num); i++) {
		sum += num[i] - 48;
	}
	if (num[strlen(num)-1] == 48 && sum % 3 == 0){
		printf("%s\n", num);
	}
	else {
		printf("-1\n");
	}
}

int compare(const void *a, const void *b)
{
	char ptA = *(int *)a;
	char ptB = *(int *)b;

	if (ptA < ptB) return 1;
	else if (ptA > ptB) return -1;
	else return 0;
}
