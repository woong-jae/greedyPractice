#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>

int compare(const void *a, const void *b)
{
	int ta = *(int *)a;
	int tb = *(int *)b;

	if (ta < tb) return -1;
	else if (ta > tb) return 1;
	else return 0;
}

int main()
{
	int n, w[1000], sum = 0;
	scanf("%d", &n);
	for (int i = 0; i < n; i++) {
		scanf(" %d", &w[i]);
	}
	qsort(w, n, sizeof(int), compare);
	for (int i = 0; i < n; i++) {
		if (w[i] > sum + 1) break;
		sum += w[i];
	}
	printf("%d\n", sum + 1);
	system("pause");
}
