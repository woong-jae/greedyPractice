//#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

void printMin(int k, char inequality[]);
void printMax(int k, char inequality[]);
int main()
{
	int k;
	char inequality[9];
	char tmp;
	scanf("%d", &k);
	for (int i = 0; i < k; i++) {
		scanf(" %c", &inequality[i]);
	}
	printMax(k, inequality);
	printMin(k, inequality);
	//system("pause");
}

void printMax(int k, char inequality[])
{
	char maxVal[11] = { NULL };
	int index = 0, bottom = 0, tmp, curNum = 9;
	while (1)
	{
		if (index == k)
		{
			for (int i = index; i >= bottom; i--) {
				maxVal[i] = 48 + curNum--;
			}
			printf("%s\n", maxVal);
			break;
		}
		if (inequality[index] == '<')
		{
			index++;
		}
		else
		{
			tmp = index;
			for (int i = index; i >= bottom; i--) {
				maxVal[i] = 48 + curNum--;
			}
			bottom = ++index;
		}
	}
}

void printMin(int k, char inequality[])
{
	char minVal[11] = { NULL };
	int index = 0, bottom = 0, tmp, curNum = 0;
	while (1)
	{
		if (index == k)
		{
			for (int i = index; i >= bottom; i--) {
				minVal[i] = 48 + curNum++;
			}
			printf("%s\n", minVal);
			break;
		}
		if (inequality[index] == '>')
		{
			index++;
		}
		else
		{
			tmp = index;
			for (int i = index; i >= bottom; i--) {
				minVal[i] = 48 + curNum++;
			}
			bottom = ++index;
		}
	}
}
